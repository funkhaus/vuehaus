{
    "name": "vuepress",
    "version": "1.1.61",
    "description": "Build smooth, responsive Wordpress sites with Vue.",
    "main": "src/main.js",
    "scripts": {
        "dev": "npm run clean && webpack --watch",
        "build": "npm run clean && NODE_ENV=production webpack",
        "test": "backstop test",
        "clean": "rimraf static/bundle.*",
        "deploy": "fh-deploy",
        "format": "prettier --write 'src/**/*+(.js|.vue|.scss)'",
        "analyze": "webpack --profile --json > bundleStats.json && webpack-bundle-analyzer bundleStats.json",
        "precommit": "pretty-quick --staged",
        "block-dev": "cross-env BABEL_ENV=default webpack --config blocks/blocks.config.js --watch",
        "block-build": "cross-env BABEL_ENV=default NODE_ENV=production webpack --config blocks/blocks.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/funkhaus/vuepress.git"
    },
    "babel": {
        "presets": [
            "env",
            "stage-0"
        ],
        "plugins": [
            "transform-runtime"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "files": [
        "blocks/**/*.*",
        "functions/**/*.*",
        "parts/**/*.*",
        "static/**/*.*",
        "functions.php",
        "index.php",
        "style.css",
        "screenshot.png"
    ],
    "author": "Funkhaus",
    "license": "ISC",
    "dependencies": {
        "auto-blur": "^1.1.1",
        "case": "^1.5.4",
        "colors": "^1.1.2",
        "fh-components": "^1.2.90",
        "imagesloaded": "^4.1.3",
        "jquery": "^3.1.1",
        "lodash": "^4.17.4",
        "scroll-to-element": "^2.0.0",
        "string.prototype.includes": "^1.0.0",
        "velocity-animate": "^1.5.0",
        "vue": "^2.5.13",
        "vue-loader": "^11.1.4",
        "vue-router": "^2.8.1",
        "vuex": "^2.5.0",
        "whatwg-fetch": "^2.0.3"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.2",
        "babel-core": "^6.26.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "backstopjs": "^3.1.21",
        "compression-webpack-plugin": "^0.3.2",
        "cross-env": "^5.0.1",
        "css-loader": "^0.26.4",
        "extract-text-webpack-plugin": "^2.1.0",
        "fh-deploy": "^1.1.5",
        "file-loader": "^0.9.0",
        "fs-extra": "^5.0.0",
        "husky": "^0.14.3",
        "node-sass": "^4.7.2",
        "optimize-css-assets-webpack-plugin": "^2.0.0",
        "postcss-import": "^9.1.0",
        "postcss-loader": "^1.3.3",
        "postcss-url": "^7.3.0",
        "prettier": "^1.14.2",
        "pretty-quick": "^1.4.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.13.1",
        "svg-inline-loader": "^0.7.1",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "url-loader": "^0.5.9",
        "vue-template-compiler": "^2.5.13",
        "webpack": "^3.1.0",
        "webpack-bundle-analyzer": "^2.11.1"
    }
}
